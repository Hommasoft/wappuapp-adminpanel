{
  "name": "wappuapp-adminpanel",
  "version": "1.0.0",
  "description": "An admin-panel for Futurice's Wappu-app",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hommasoft/wappuapp-adminpanel.git"
  },
  "author": "Hommasoft",
  "license": "MIT",
  "homepage": "https://github.com/Hommasoft/wappuapp-adminpanel#readme",
  "devDependencies": {
    "husky": "0.14.3"
  },
  "scripts": {
    "start": "cd wappuapp-adminpanel && npm run start",
    "build":
      "cd wappuapp-adminpanel && npm run build && cp -r ./build ../build",
    "postinstall": "cd wappuapp-adminpanel && npm install",
    "precommit": "cd wappuapp-adminpanel && npm run precommit",
    "lint": "cd wappuapp-adminpanel && npm run eslint"
  }
}
